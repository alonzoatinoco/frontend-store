-var navegadores = ["Tienda","Nosotros"];
-var subtitulos = ["Dise√±o Web", "Aplicaciones Web", "E-comerce"];
-var url = ["index.html", "nosotros.html"];
<!--mixin para secccion Mis Servicios-->
mixin caja(subtitulos,imagen,contenido,)
	section.caja__contenido
		h3=subtitulos
		.caja__contenido--images
			i(class=imagen)
		p=contenido

mixin nav(nav, enlace,)
		a(class="nav__enlace" href=enlace)=nav

doctype html
html(lang="es")
	head
		title Frontend Store
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(name='viewport', content='width=device-width, maximum-scale=1.0, user-scalable=no')
		link(rel='stylesheet', href='../css/principal.css')
		link(rel="stylesheet", href="https://fonts.googleapis.com/css2?family=Staatliches&display=swap")
		link(rel="stylesheet",href="https://fonts.googleapis.com/css?family=Lato:400,400i,700,700i|Oswald:400,500,600,700&display=swap" )
		script(src='https://kit.fontawesome.com/552ebddad2.js')
	body